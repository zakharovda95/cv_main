{
  "name": "cv-main",
  "private": false,
  "type": "module",
  "scripts": {
    "first-run:init": "setup-project.sh",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:host": "nuxt dev --host 0.0.0.0 --port 3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lintfix": "eslint . --fix && prettier --write --list-different --config .prettierrc.config.mjs .",
    "default:git-hook-path": "git config core.hooksPath .git/hooks || echo 'error'",
    "show:git-hook-path": "git rev-parse --git-path hooks"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@nuxtjs/svg-sprite": "^1.0.2",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^10.10.0",
    "@vueuse/nuxt": "^10.10.0",
    "gsap": "^3.12.5",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt-lodash": "^2.5.3",
    "prettier": "^3.3.0",
    "sass": "^1.77.4",
    "typescript": "^5.4.5"
  }
